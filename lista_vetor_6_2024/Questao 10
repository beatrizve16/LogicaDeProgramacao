#include <stdio.h>
#include <string.h>

int main() {
    char palavra[100];
    int i, comprimento;

    printf("Digite uma palavra: ");
    scanf("%s", palavra);

    comprimento = strlen(palavra);

    printf("Palavra na ordem inversa: ");
    for (i = comprimento - 1; i >= 0; i--) {
        printf("%c", palavra[i]);
    }

    return 0;
}
