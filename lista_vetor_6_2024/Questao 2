#include <stdio.h>

int main() {
    int vetor[4];
    int i;

    printf("Digite quatro valores:\n");

    for (i = 0; i < 4; i++) {
        scanf("%d", &vetor[i]);
    }

    printf("Valores armazenados no vetor (em ordem inversa):\n");

    for (i = 3; i >= 0; i--) {
        printf("%d ", vetor[i]);
    }

    return 0;
}
